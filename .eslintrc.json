{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "jsx-a11y/media-has-caption": 0,
        "react/jsx-indent": 0,
        "no-nested-ternary": 0,
        "react/prop-types": 0,
        "react/self-closing-comp": 0,
        "react/jsx-closing-tag-location": 0,
        "consistent-return": 0,
        "no-shadow": [
            2,
            {
                "hoist": "all",
                "allow": [
                    "resolve",
                    "reject",
                    "done",
                    "next",
                    "err",
                    "error"
                ]
            }
        ],
        "react/no-unescaped-entities": 0,
        "import/prefer-default-export": 0,
        "no-unused-vars": [
            1
        ],
        "no-debugger": 0,
        "no-alert": 0,
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-props-no-spreading": 0
       
    }
}
